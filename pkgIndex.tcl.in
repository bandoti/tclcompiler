# Tcl package index file, version 1.1
#
if {[package vsatisfies [package provide Tcl] 9.0-]} {
    package ifneeded compiler @PACKAGE_VERSION@ \
	    [list load [file join $dir @PKG_LIB_FILE@]]
} else {
    # Note: "compiler" is hardcoded here to satisfy the exported symbols
    # baked in the so/dll.
    package ifneeded compiler @PACKAGE_VERSION@ \
	    [list load [file join $dir @PKG_LIB_FILE@]]
}
